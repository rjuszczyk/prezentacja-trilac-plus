<?xml version="1.0" encoding="utf-8"?>
<pl.pharmaway.prezentacjatrilacplus.view.FixedScaledLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:fixedWidth="1366dp"
    app:fixedHeight="768dp"
    >

    <pl.pharmaway.prezentacjatrilacplus.view.FixedImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:imageReference="@drawable/page2"
        />

    <View
        android:id="@+id/skutecznosc"
        android:layout_width="390dp"
        android:layout_height="140dp"
        android:layout_marginLeft="570dp"
        android:layout_marginTop="200dp"
        />

    <View
        android:id="@+id/optymalna_kolonizacja"
        android:layout_width="390dp"
        android:layout_height="140dp"
        android:layout_marginLeft="400dp"
        android:layout_marginTop="400dp"
        />

    <View
        android:id="@+id/bezpieczenstwo"
        android:layout_width="390dp"
        android:layout_height="140dp"
        android:layout_marginLeft="200dp"
        android:layout_marginTop="590dp"
        />

    <View
        android:id="@+id/button_next"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_gravity="right|bottom"
        />
</pl.pharmaway.prezentacjatrilacplus.view.FixedScaledLayout>